<Application x:Class="Stack_Center.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./lang/lang.en-US.xaml"></ResourceDictionary>
                <ResourceDictionary Source="./lang/lang.sr.xaml"></ResourceDictionary>
                <ResourceDictionary x:Name="DayTheme" Source="./Themes/DayTheme.xaml"></ResourceDictionary>
                <ResourceDictionary  x:Name="NightTheme" Source="./Themes/NightTheme.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>